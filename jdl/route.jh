entity Route {
	routeName String required
}

entity Location {
	locationName String required
}

entity Coordinate {
	longitude Long required
    latitude Long required
}

relationship OneToOne{
	Route{startLocation} to Location
    Route{endLocation} to Location
    Location{coordinate} to Coordinate
    Coordinate{location} to Location
}

relationship OneToMany{
	Route{coordinates} to Coordinate
}
