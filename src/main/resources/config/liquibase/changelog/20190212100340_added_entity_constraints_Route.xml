<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Route.
    -->
    <changeSet id="20190212100340-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="start_location_id"
                                 baseTableName="route"
                                 constraintName="fk_route_start_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="end_location_id"
                                 baseTableName="route"
                                 constraintName="fk_route_end_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="routes_id"
                                 baseTableName="route_location"
                                 constraintName="fk_route_location_routes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="route"/>
        <addForeignKeyConstraint baseColumnNames="locations_id"
                                 baseTableName="route_location"
                                 constraintName="fk_route_location_locations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>
        
    </changeSet>
</databaseChangeLog>
